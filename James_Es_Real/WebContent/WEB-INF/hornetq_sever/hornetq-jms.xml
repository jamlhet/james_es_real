<configuration xmlns="urn:hornetq"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="urn:hornetq /schema/hornetq-jms.xsd">

   <connection-factory name="NettyConnectionFactory">
      <xa>true</xa>
      <connectors>
         <connector-ref connector-name="netty"/>
      </connectors>
      <entries>
         <entry name="/XAConnectionFactory"/>
      </entries>
   </connection-factory>
   
   <connection-factory name="NettyConnectionFactory">
      <xa>false</xa>
      <connectors>
         <connector-ref connector-name="netty"/>
      </connectors>
      <entries>
         <entry name="/ConnectionFactory"/>
      </entries>
   </connection-factory>
   
   <connection-factory name="NettyThroughputConnectionFactory">
      <xa>true</xa>
      <connectors>
         <connector-ref connector-name="netty-throughput"/>
      </connectors>
      <entries>
         <entry name="/XAThroughputConnectionFactory"/>
      </entries>
   </connection-factory>
   
   <connection-factory name="NettyThroughputConnectionFactory">
      <xa>false</xa>
      <connectors>
         <connector-ref connector-name="netty-throughput"/>
      </connectors>
      <entries>
         <entry name="/ThroughputConnectionFactory"/>
      </entries>
   </connection-factory>

   <queue name="DLQ">
      <entry name="/queue/DLQ"/>
   </queue>
   
   <queue name="ExpiryQueue">
      <entry name="/queue/ExpiryQueue"/>
   </queue>

  <queue name="marceQueue">
      <entry name="/queue/exampleQueue"/>
   </queue>

   <queue name="facturaElectronicaQueue">
      <entry name="/queue/facturaElectronicaQueue"/>
   </queue>
   
   <queue name="logElectronicoQueue">
      <entry name="/queue/logElectronicoQueue"/>
   </queue>
   
   <queue name="logComputadorQueue">
      <entry name="/queue/logComputadorQueue"/>
   </queue>
     
   <queue name="logCuentaCobroQueue">
      <entry name="/queue/logCuentaCobroQueue"/>
   </queue>
 
   <!--    colas para de emision de documentos -->  
   <queue name="eventualCuentaCobroEmitirQueue">
      <entry name="/queue/eventualCuentaCobroEmitirQueue"/>
   </queue>
   
   <queue name="eventualElectronicaEmitirQueue">
      <entry name="/queue/eventualElectronicaEmitirQueue"/>
   </queue>
   
   <queue name="eventualComputadorEmitirQueue">
      <entry name="/queue/eventualComputadorEmitirQueue"/>
   </queue>
 
   <!--    colas para aceptar documentos -->  
   <queue name="eventualCuentaCobroAceptarQueue">
      <entry name="/queue/eventualCuentaCobroAceptarQueue"/>
   </queue>
   
   <queue name="eventualElectronicaAceptarQueue">
      <entry name="/queue/eventualElectronicaAceptarQueue"/>
   </queue>
   
   <queue name="eventualComputadorAceptarQueue">
      <entry name="/queue/eventualComputadorAceptarQueue"/>
   </queue>
   
   <!--    colas para rechazar documentos -->  
   <queue name="eventualCuentaCobroRechazarQueue">
      <entry name="/queue/eventualCuentaCobroRechazarQueue"/>
   </queue>
   
   <queue name="eventualElectronicaRechazarQueue">
      <entry name="/queue/eventualElectronicaRechazarQueue"/>
   </queue>
   
   <queue name="eventualComputadorRechazarQueue">
      <entry name="/queue/eventualComputadorRechazarQueue"/>
   </queue>
   
   <!--    colas para anular documentos -->  
   <queue name="eventualCuentaCobroAnularQueue">
      <entry name="/queue/eventualCuentaCobroAnularQueue"/>
   </queue>
   
   <queue name="eventualElectronicaAnularQueue">
