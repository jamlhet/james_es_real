   <!-- Colas para los reportes personalizados -->
   <queue name="generarReporteQueue">
      <entry name="/queue/generarReporteQueue"/>
   </queue>
   
   <queue name="actualizarReporteQueue">
      <entry name="/queue/actualizarReporteQueue"/>
   </queue>
   
   <!-- Cola para el batch de adquirientes -->
   <queue name="actualizarBatchAdquirienteQueue">
