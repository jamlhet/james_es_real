<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CertiFactura_Eventual</display-name>
  
  <context-param>
	   <param-name>webAppRootKey</param-name>
	   <param-value>webapp.eventual</param-value>
  </context-param>
	  
  <context-param>
	  <param-name>log4jConfigLocation</param-name>
	  <param-value>/WEB-INF/log4j/log4j.xml</param-value>
  </context-param>

  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>/WEB-INF/spring/spring-jms-beans.xml</param-value>
  </context-param>
  
  <listener>
        <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
  </listener>
  
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  
  <listener>
    <listener-class>com.certicamara.certifactura.eventual.contextListener.ListenerContexto</listener-class>
 </listener>
</web-app>